{
  "name": "@dwex/cli",
  "version": "1.0.0-beta.13",
  "description": "CLI tool for building and developing Dwex applications",
  "type": "module",
  "bin": {
    "dwex": "./dist/index.js"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "@dwex/source": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "templates",
    "!**/*.tsbuildinfo"
  ],
  "dependencies": {
    "tslib": "^2.3.0",
    "commander": "^12.1.0",
    "chalk": "^5.3.0",
    "ora": "^8.1.1",
    "@clack/prompts": "^0.8.2",
    "ejs": "^3.1.10",
    "picocolors": "^1.1.1"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/ejs": "^3.1.5"
  }
}
